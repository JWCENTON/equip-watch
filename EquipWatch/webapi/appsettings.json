{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MySQL",
        "Args": {
          "connectionString": "Server=equipwatchdb.mysql.database.azure.com;Database=serilog;",
          "tableName": "serilog",
          "autoCreateSqlTable": true
        }
      },
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "MySqlContextConnection": "Server=equipwatchdb.mysql.database.azure.com;Database=equipwatch;",
    "MySqlIdentityContextConnection": "Server=equipwatchdb.mysql.database.azure.com;Database=identityequipwatch;"
  },
